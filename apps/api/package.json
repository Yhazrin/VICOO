{
  "name": "vicoo-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit --pretty false",
    "test": "pnpm test:smoke",
    "test:smoke": "node --test tests/smoke.test.mjs",
    "start": "node dist/index.js",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "test:auth": "node --import tsx --test src/middleware/auth.test.ts"
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/utils": "^10.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-graphql": "^0.12.0",
    "graphql": "^16.9.0",
    "multer": "^1.4.5-lts.1",
    "music-metadata": "^11.12.0",
    "node-pty": "^1.1.0",
    "sql.js": "^1.11.0",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.0",
    "@types/sql.js": "^1.4.9",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.0",
    "typescript": "~5.8.2"
  }
}
