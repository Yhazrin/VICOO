  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "Note": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "title": { "type": "string" },
          "category": { "type": "string", "enum": ["code", "design", "meeting", "idea"] },
          "snippet": { "type": "string" },
          "tags": { "type": "array", "items": { "type": "string" } },
          "timestamp": { "type": "string", "format": "date-time" },
          "content": { "type": "string" },
          "summary": { "type": "string" },
          "published": { "type": "boolean" },
          "coverImage": { "type": "string" },
          "color": { "type": "string" },
          "icon": { "type": "string" }
        }
      },
      "NoteCreate": {
        "type": "object",
        "required": ["title"],
        "properties": {
          "title": { "type": "string" },
          "category": { "type": "string", "enum": ["code", "design", "meeting", "idea"], "default": "idea" },
          "tags": { "type": "array", "items": { "type": "string" }, "default": [] },
          "content": { "type": "string", "default": "" },
          "snippet": { "type": "string" },
          "published": { "type": "boolean", "default": false },
          "color": { "type": "string" },
          "icon": { "type": "string" }
        }
      },
      "NoteUpdate": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "category": { "type": "string", "enum": ["code", "design", "meeting", "idea"] },
          "tags": { "type": "array", "items": { "type": "string" } },
          "content": { "type": "string" },
          "snippet": { "type": "string" },
          "published": { "type": "boolean" },
          "coverImage": { "type": "string" },
          "color": { "type": "string" },
          "icon": { "type": "string" }
        }
      },
      "NoteListResponse": {
        "type": "object",
        "properties": {
          "data": { "type": "array", "items": { "$ref": "#/components/schemas/Note" } },
          "meta": {
            "type": "object",
            "properties": {
              "total": { "type": "integer" },
              "limit": { "type": "integer" },
              "offset": { "type": "integer" }
            }
          }
        }
      },
      "NoteResponse": {
        "type": "object",
        "properties": {
          "data": { "$ref": "#/components/schemas/Note" }
        }
      },
      "Tag": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "name": { "type": "string" },
          "color": { "type": "string" },
          "count": { "type": "integer" }
        }
      },
      "TagCreate": {
        "type": "object",
        "required": ["name"],
        "properties": {
          "name": { "type": "string" },
          "color": { "type": "string" }
        }
      },
      "TagUpdate": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "color": { "type": "string" }
        }
      },
      "Node": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "x": { "type": "number" },
          "y": { "type": "number" },
          "label": { "type": "string" },
          "type": { "type": "string", "enum": ["planet", "moon"] },
          "color": { "type": "string" },
          "icon": { "type": "string" },
          "description": { "type": "string" },
