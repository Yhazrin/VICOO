              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "ok": { "type": "boolean" },
                        "timestamp": { "type": "string", "format": "date-time" },
                        "version": { "type": "string" }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
